CREATE TABLE NOxAnalysis (
      fileName TEXT NOT NULL,
      nox REAL,
      analyserVersion INTEGER AS Int NOT NULL,
      PRIMARY KEY (fileName)
);

selectAll:
SELECT *
FROM NOxAnalysis;

insert:
INSERT INTO NOxAnalysis(fileName, nox, analyserVersion)
VALUES (?,?,?);

selectByName:
SELECT *
FROM NOxAnalysis
WHERE fileName = ?;