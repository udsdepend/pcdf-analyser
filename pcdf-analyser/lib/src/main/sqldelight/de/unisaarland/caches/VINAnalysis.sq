CREATE TABLE VINAnalysis (
  fileName TEXT NOT NULL,
  VIN TEXT,
  analyserVersion INTEGER AS Int NOT NULL,
  PRIMARY KEY (fileName)
);

selectAll:
SELECT *
FROM VINAnalysis;

insert:
INSERT INTO VINAnalysis(fileName, VIN, analyserVersion)
VALUES (?, ? ,?);

selectByName:
SELECT *
FROM VINAnalysis
WHERE fileName = ?;
